"#          /dev/discs/disc0/part1#/dev/discs/disc1/part1    \\\n"
"#    .                                                      \\\n"
"#    1 2 0 swap -                                           \\\n"
"#          /dev/discs/disc0/part5#/dev/discs/disc1/part5    \\\n"
"#    .                                                      \\\n"
"#    0 2 0 ext3 /home                                       \\\n"
"#          /dev/discs/disc0/part6#/dev/discs/disc1/part6    \\\n"
