#, no-c-format
msgid "ATA%s (%s)"
msgstr "ATA%s (%s)"

#. Type: text
#. Description
#. For example ATA1, partition #5 (ad0s5)
#. :sl1:
#: ../partman-base.templates:42001
#, no-c-format
msgid "ATA%s, partition #%s (%s)"
msgstr "ATA%s, partició no. %s (%s)"

#. Type: text
#. Description
#. For example IDE0 master (hda)
#. :sl1:
#: ../partman-base.templates:43001
#, no-c-format
