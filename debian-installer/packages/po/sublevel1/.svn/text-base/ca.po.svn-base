msgid "Cancel"
msgstr "Canceŀla"

#. Type: text
#. Description
#. :sl1:
#: ../partman-base.templates:1001
msgid "Starting up the partitioner"
msgstr "S'està iniciant el partidor"

#. #-#-#-#-#  templates.pot (PACKAGE VERSION)  #-#-#-#-#
#. Type: text
#. Description
#. :sl1:
#. Type: text
#. Description
#. :sl2:
