#. Type: text
#. Description
#. :sl1:
#: ../partman-base.templates:1001
msgid "Starting up the partitioner"
msgstr "Duke nisur ndarësin"

#. #-#-#-#-#  templates.pot (PACKAGE VERSION)  #-#-#-#-#
#. Type: text
#. Description
#. :sl1:
#. Type: text
#. Description
