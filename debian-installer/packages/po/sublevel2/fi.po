#: ../netcfg-common.templates:32001
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:33001
msgid "USB net"
msgstr "USB-verkko"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:35001
msgid "Serial-line IP"
msgstr "Sarjalinja-IP"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:36001
