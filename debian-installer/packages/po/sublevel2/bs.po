msgstr "Ethernet"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:32001
msgid "Token Ring"
msgstr "Token Ring"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:33001
msgid "USB net"
msgstr "USB mreža"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:35001
msgid "Serial-line IP"
msgstr "Serial-line IP"

#. Type: text
#. Description
#. :sl2:
#: ../netcfg-common.templates:36001
